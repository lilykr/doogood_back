{
  "name": "low-impact-server",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index",
  "scripts": {
    "compile": "tsc -p tsconfig.build.json",
    "build": "yarn run clean && yarn run compile",
    "start": "node build",
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --rs src",
    "lint": "eslint src tests --fix --ext .js,.jsx,.ts,.tsx,.json --no-error-on-unmatched-pattern",
    "ts-check": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@keystonejs/adapter-mongoose": "^11.2.0",
    "@keystonejs/app-graphql": "6.2.0",
    "@keystonejs/auth-password": "6.0.1",
    "@keystonejs/fields": "22.0.0",
    "@keystonejs/keystone": "19.3.1",
    "@keystonejs/list-plugins": "8.1.0",
    "@la-forge-dev/eslint-config-tsx": "^1.1.4",
    "chalk": "^4.1.0",
    "crypto": "^1.0.1",
    "eslint": "^7.23.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "cross-env": "^7.0.3",
    "graphql": "^15.5.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": ">=3.3.1 and <4.1.0"
  }
}
