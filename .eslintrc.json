{
    "parser": "babel-eslint",
    "extends": "@la-forge-dev/eslint-config-tsx",
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true,
        "browser": false
    },
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": false
        },
        "sourceType": "module"
    },
    "globals": {
        "strapi": true
    },
}